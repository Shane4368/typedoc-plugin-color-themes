{
    "name": "typedoc-plugin-themes",
    "version": "0.1.0",
    "description": "A TypeDoc plugin that modifies the colors of the builtin themes and adds more to select from.",
    "exports": "./src/index.js",
    "files": [
        "src/*.js",
        "src/*.css",
        "src/*.xml"
    ],
    "scripts": {
        "prepare": "npm run-script compile",
        "compile": "sass src/themes.scss:src/themes.css --no-source-map",
        "docs": "typedoc"
    },
    "keywords": [
        "typedoc-plugin"
    ],
    "author": "Shane4368",
    "license": "MIT",
    "devDependencies": {
        "dummy-typescript-project": "github:shane4368/dummy-typescript-project",
        "sass": "^1.55.0",
        "typedoc": "^0.23.15"
    },
    "private": true
}